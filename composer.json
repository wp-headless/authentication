{
    "name": "wp-headless/authentication",
    "type": "library",
    "description": "Wordpress JWT Authentication for JSON API",
    "homepage": "https://github.com/wp-headless/authentication",
    "keywords": [
        "wordpresss",
        "API",
        "JWT",
        "authentication",
        "oauth",
        "oauth2"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew McLagan",
            "email": "andrewmclagan@gmail.com"
        }
    ],
    "require": {
        "fzaninotto/faker": "^1.8",
        "illuminate/support": "^5.8",
        "league/oauth2-server": "^8.0",
        "paragonie/easydb": "^2",
        "php-di/php-di": "^6.0",
        "phpseclib/phpseclib": "^2.0",
        "tightenco/collect": "^5.8"
    },
    "require-dev": {
        "wp-phpunit/wp-phpunit": "^5.3",
        "roots/wordpress": "5.3.0",
        "phpunit/phpunit": "^7",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "WPHeadless\\JWTAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-update-cmd": "mkdir -p wordpress/wp-content/uploads"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "wordpress-install-dir": "wordpress"
    }
}
